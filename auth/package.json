    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.11",
