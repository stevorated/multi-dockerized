{
  "name": "auth",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production yarn build && node ./dist/bundle.min.js",
    "dev": "yarn build && concurrently \"yarn watch:build\" \"yarn watch:auth\"",
    "watch:build": "webpack --watch --host 0.0.0.0",
    "watch:auth": "nodemon \"./dist/bundle.min.js\" --watch \"./dist\" ",
    "build": "webpack",
    "clean": "rimraf ./dist",
    "clean:mod": "rimraf ./node_modules",
    "test": "yarn jest --watchAll",
    "compose": "docker-compose down && docker-compose up -d pgadmin redis postgres && docker-compose up",
    "concurrently": "concurrently --help"
  },
