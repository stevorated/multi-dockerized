{
    "name": "fib",
    "description": "fibo api",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "cross-env NODE_ENV=production yarn build && node ./dist/bundle.min.js",
        "dev": "yarn build && concurrently \"yarn watch:build\" \"yarn watch:api\"",
        "watch:api": "nodemon \"./dist/bundle.min.js\" --watch \"./dist\" ",
        "watch:build": "webpack --watch --host 0.0.0.0",
        "build": "webpack",
        "clean": "rimraf ./dist",
        "clean:mod": "rimraf ./node_modules",
        "test": "yarn jest --watchAll"
    },
    "dependencies": {
        "await-to-js": "^2.1.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "helmet": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "morgan": "^1.10.0",
        "pg": "^8.3.0",
        "redis": "^3.0.2",
        "rimraf": "^3.0.2",
        "strong-error-handler": "^3.5.0",
        "typeorm": "^0.2.25",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.7",
        "@types/helmet": "^0.0.48",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.161",
        "@types/morgan": "^1.9.1",
        "@types/node": "^14.0.27",
        "@types/pg": "^7.14.4",
        "@types/redis": "^2.8.25",
        "@types/strong-error-handler": "^2.3.1",
        "@types/supertest": "^2.0.10",
        "@types/winston": "^2.4.4",
        "awesome-typescript-loader": "^5.2.1",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.6.0",
        "jest": "^26.4.0",
        "jest-junit": "^11.1.0",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2",
        "ts-jest": "^26.2.0",
        "ts-loader": "^8.0.2",
        "ts-node": "^8.10.2",
        "tsc-watch": "^4.2.9",
